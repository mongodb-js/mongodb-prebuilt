{
  "name": "mongodb-prebuilt",
  "version": "6.5.0",
  "description": "Install MongoDB prebuilt binaries via npm",
  "main": "built/mongodb-prebuilt-all",
  "types": "built/mongodb-prebuilt-all.d.ts",
  "scripts": {
    "cache-clean": "rm -rf dist *.txt",
    "prepare": "npm run clean && npm run tsc",
    "tsc": "rm -rf built && tsc",
    "clean": "rm -rf built",
    "lint-ts": "tslint -c tslint.json './src/**/*.ts' --fix"
  },
  "bin": {
    "bsondump": "built/bin/bsondump.js",
    "mongo": "built/bin/mongo.js",
    "mongod": "built/bin/mongod.js",
    "mongodump": "built/bin/mongodump.js",
    "mongoexport": "built/bin/mongoexport.js",
    "mongofiles": "built/bin/mongofiles.js",
    "mongoimport": "built/bin/mongoimport.js",
    "mongooplog": "built/bin/mongooplog.js",
    "mongoperf": "built/bin/mongoperf.js",
    "mongorestore": "built/bin/mongorestore.js",
    "mongos": "built/bin/mongos.js",
    "mongosniff": "built/bin/mongosniff.js",
    "mongostat": "built/bin/mongostat.js",
    "mongotop": "built/bin/mongotop.js",
    "mongo-supervise": "built/bin/mongo-supervise.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/winfinit/mongodb-prebuilt.git"
  },
  "keywords": [
    "mongodb",
    "prebuilt",
    "mongod",
    "mongo",
    "nosql",
    "database",
    "mongodb-prebuilt",
    "mongodb prebuilt"
  ],
  "author": "Roman Jurkov <winfinit@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/winfinit/mongodb-prebuilt/issues"
  },
  "dependencies": {
    "debug": "^2.2.0",
    "glob": "^7.1.1",
    "mongodb-download": "^2.2.7",
    "spawn-sync": "1.0.15",
    "yargs": "^3.26.0"
  },
  "devDependencies": {
    "@types/node": "^6.0.58",
    "chai": "^3.5.0",
    "tsc": "^1.20150623.0",
    "typescript": "^2.9.2"
  },
  "homepage": "https://github.com/winfinit/mongodb-prebuilt#readme"
}
