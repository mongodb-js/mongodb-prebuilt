{
  "name": "mongodb-prebuilt",
  "version": "6.3.5",
  "description": "Install MongoDB prebuilt binaries via npm",
  "main": "dist/mongodb-prebuilt-all",
  "types": "dist/mongodb-prebuilt-all.d.ts",
  "scripts": {
    "prepublish": "yarn run compile",
    "compile": "tsc",
    "cache-clean": "rm -rf dist *.txt"
  },
  "bin": {
    "bsondump": "dist/bin/bsondump.js",
    "mongo": "dist/bin/mongo.js",
    "mongod": "dist/bin/mongod.js",
    "mongodump": "dist/bin/mongodump.js",
    "mongoexport": "dist/bin/mongoexport.js",
    "mongofiles": "dist/bin/mongofiles.js",
    "mongoimport": "dist/bin/mongoimport.js",
    "mongooplog": "dist/bin/mongooplog.js",
    "mongoperf": "dist/bin/mongoperf.js",
    "mongorestore": "dist/bin/mongorestore.js",
    "mongos": "dist/bin/mongos.js",
    "mongosniff": "dist/bin/mongosniff.js",
    "mongostat": "dist/bin/mongostat.js",
    "mongotop": "dist/bin/mongotop.js",
    "mongo-supervise": "dist/bin/mongo-supervise.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/winfinit/mongodb-prebuilt.git"
  },
  "keywords": [
    "mongodb",
    "prebuilt",
    "mongod",
    "mongo",
    "nosql",
    "database",
    "mongodb-prebuilt",
    "mongodb prebuilt"
  ],
  "author": "Roman Jurkov <winfinit@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/winfinit/mongodb-prebuilt/issues"
  },
  "dependencies": {
    "debug": "^2.2.0",
    "glob": "^7.1.1",
    "mongodb-download": "^2.2.6",
    "spawn-sync": "1.0.15",
    "yargs": "^3.26.0"
  },
  "devDependencies": {
    "@types/debug": "^0.0.30",
    "@types/node": "^6.0.58",
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "ts-node": "^7.0.0",
    "typescript": "^2.9.2"
  },
  "homepage": "https://github.com/winfinit/mongodb-prebuilt#readme"
}
